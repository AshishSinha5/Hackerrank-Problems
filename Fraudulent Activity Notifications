int max(int arr[],int n)
    {
    int location,maximum,c;
  maximum = INT_MIN;
   
 
  for (c = 0; c < n; c++)
  {
    if (arr[c] > maximum)
    {
       maximum  = arr[c];
       location = c+1;
    }
  }
   
    return arr[location-1];
}

